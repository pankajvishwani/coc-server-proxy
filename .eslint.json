{
	"parser": "babel-eslint",
	"parserOptions": {
		"ecmaVersion": 7,
		"sourceType": "module"
	},
	"rules": {
		"semi": 2,
		"no-console": "off"
	},
	"plugins": [],
	"extends": ["eslint:recommended"],
	"globals": {
		"console": true,
		"alert": true,
		"document": true,
		"__dirname": true,
		"require": true,
		"window": true,
		"process": true,
		"module": true,
		"define": true,
		"_": true,
		"Promise": true,
		"setTimeout": true,
		"clearTimeout": true
	}
}
